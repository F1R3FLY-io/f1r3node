new
  VaultAddress(`rho:vault:address`), SystemVaultCh,
  vaultAddrCh,
  stdout(`rho:io:stdout`)
in {

  stdout!(("1.know_ones_vaultaddress.rho")) |

  // REPLACE THE PUB_KEY HERE vvv
  match "%PUB_KEY" {
    pubKey => {

      VaultAddress!("fromPublicKey", pubKey.hexToBytes(), *vaultAddrCh) |

      for (@vaultAddress <- vaultAddrCh) {
        stdout!(("VaultAddress for pubKey", pubKey, "is", vaultAddress))
      }
    }
  }

}

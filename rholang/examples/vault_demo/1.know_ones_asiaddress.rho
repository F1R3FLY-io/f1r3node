new
  ASIAddress(`rho:asi:address`), ASIVaultCh,
  asiAddrCh,
  stdout(`rho:io:stdout`)
in {

  stdout!(("1.know_ones_revaddress.rho")) |

  // REPLACE THE PUB_KEY HERE vvv
  match "%PUB_KEY" {
    pubKey => {

      ASIAddress!("fromPublicKey", pubKey.hexToBytes(), *asiAddrCh) |

      for (@asiAddress <- asiAddrCh) {
        stdout!(("ASIAddress for pubKey", pubKey, "is", asiAddress))
      }
    }
  }

}
